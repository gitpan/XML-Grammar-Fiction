+
+# TEST
+ok(1, 'Loaded fine');
+
+my $p = XML::LibXML->new();
+# TEST
+ok ($p, 'Can initialize a new XML::LibXML instance');
+
+my ($runtime_version) = (XML::LibXML::LIBXML_RUNTIME_VERSION() =~ /\A(\d+)/);
+
+# TEST
+if (!is (
+    XML::LibXML::LIBXML_VERSION, $runtime_version,
+    'LIBXML__VERSION == LIBXML_RUNTIME_VERSION',
