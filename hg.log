-                push @attrs, { 'key' => $1, 'value' => $2, };
-            }
-
-            my $is_standalone = 0;
-            if ($$l =~ m{\G\s*/\s*>}cg)
-            {
-                $is_standalone = 1;
-            }
-            elsif ($$l !~ m{\G>}g)
-            {
-                Carp::confess (
-                    "Cannot match the \">\" of the opening tag at line " 
-                        . $self->_get_line_num()
-                );
-            }
-            
-            return
-            {
-                name => $id,
-                is_standalone => $is_standalone,
-                line => $self->_get_line_num(),
-                attrs => \@attrs,
-            };
-        }
-    );
