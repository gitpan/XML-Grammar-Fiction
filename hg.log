+{
+   diag("DO NOT REPORT THIS FAILURE: Your setup of library paths is incorrect!");
+}
+
+diag( "\n\nCompiled against libxml2 version: ",XML::LibXML::LIBXML_VERSION,
+     "\nRunning libxml2 version:          ",$runtime_version,
+     "\n\n");

